package com.test;

public class EchoBean {
	public String sayEcho() {

		return "안녕하세요 ";
	}

	public EchoBean() {
	
		// TODO Auto-generated constructor stub
		System.out.println("echobean 생성자");
		
	}
	public EchoBean(String n) {
	
		// TODO Auto-generated constructor stub
		System.out.println("echobean 생성자 + "+n);
		
	}
}
