package com.test;

public class EchoBean {
	private String mesg;
	public String sayEcho() {

		return "안녕하세요 ";
	}

	public EchoBean() {
	
		// TODO Auto-generated constructor stub
		System.out.println("echobean 생성자");
		
	}
	public EchoBean(String mesg) {
	this.mesg=mesg;
		// TODO Auto-generated constructor stub
		System.out.println("echobean 생성자 + "+mesg);
		
	}
}
