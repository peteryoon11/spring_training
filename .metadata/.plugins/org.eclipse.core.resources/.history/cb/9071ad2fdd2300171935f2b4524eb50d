package com.test;

public class Person {

	private String name;
	private int age;

	public String getEmail() {
		return name;
	}

	public void setEmail(String email) {
		this.name = email;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public Person() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Person(String email, int age) {
		super();
		this.name = email;
		this.age = age;
	}

}
