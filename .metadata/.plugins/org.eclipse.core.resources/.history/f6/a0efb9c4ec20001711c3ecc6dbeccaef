package com.service;

import com.dao.DeptDAO;

public class DeptService {
	private DeptDAO dao;

	public DeptDAO getDao() {
		return dao;
	}

	public void setDao(DeptDAO dao) {
		this.dao = dao;
	}

	public DeptService() {
		super();
		// TODO Auto-generated constructor stub
	}

	public List<DeptDTO> select() {
		return dao.select();
	}

}
